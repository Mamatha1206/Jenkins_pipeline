pipeline {
    agent any
    
    stages {
        stage('Clone Repository') {
            steps {
                git (url:'hhtps://github.com/Mamatha1206/jenkins_pipeline.git'

            }
        }
        
        stage('Run Code') {
            steps {
                sh 'python3.fist.py'
            }
        }
        
        stage('Test') {
            steps {
                sh 'echo Running tests...'
            }
        }
        
        stage('Archive Artifacts') {
            steps {
                archiveArtifacts artifacts: '**/target/*.jar', fingerprint: true
            }
        }
    }
}
